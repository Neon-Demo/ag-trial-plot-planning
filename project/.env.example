# Authentication
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your-secret-key-here

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Microsoft OAuth
MICROSOFT_CLIENT_ID=your-microsoft-client-id
MICROSOFT_CLIENT_SECRET=your-microsoft-client-secret
MICROSOFT_TENANT_ID=optional-tenant-id

# Demo mode settings
ALLOW_DEMO_LOGIN=true
# Optional: if you want to use both demo login AND database at the same time
USE_DB_WITH_DEMO=false

# Database connection (optional if using demo mode only)
# DATABASE_URL=postgresql://user:password@localhost:5432/ag_trial_plot

# Debug settings
DEBUG_AUTH=false

# Optional: Weather API keys
# WEATHER_API_KEY=your-weather-api-key